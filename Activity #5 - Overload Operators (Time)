using System;

public struct Time
{
    private readonly int minutes;

    public Time(int hh, int mm)
    {
        this.minutes = 60 * hh + mm;
    }

    public int Hour => minutes / 60;
    public int Minute => minutes % 60;

    public override string ToString()
    {
        return string.Format("{0:D2}:{1:D2}", Hour, Minute);
    }

    public static Time operator +(Time t1, Time t2)
    {
        return new Time(0, t1.minutes + t2.minutes);
    }

    public static Time operator -(Time t1, Time t2)
    {
        return new Time(0, t1.minutes - t2.minutes);
    }

    public static bool operator ==(Time t1, Time t2)
    {
        return t1.minutes == t2.minutes;
    }

    public static bool operator !=(Time t1, Time t2)
    {
        return !(t1 == t2);
    }

    public override bool Equals(object obj)
    {
        if (obj is Time)
            return this == (Time)obj;
        return false;
    }

    public override int GetHashCode()
    {
        return minutes;
    }
}

class Program
{
    static void Main(string[] args)
    {
        Time t1 = new Time(10, 30);
        Time t2 = new Time(2, 45);

        Time sum = t1 + t2;
        Time diff = t1 - t2;

        Console.WriteLine("Time 1: " + t1);
        Console.WriteLine("Time 2: " + t2);
        Console.WriteLine("Sum: " + sum);
        Console.WriteLine("Difference: " + diff);
        Console.WriteLine("Equal? " + (t1 == t2));
        Console.WriteLine("Not Equal? " + (t1 != t2));

        Console.ReadLine();
    }
}
